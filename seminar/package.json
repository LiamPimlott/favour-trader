{
  "name": "react-counter-boilerplate",
  "version": "0.2.0",
  "description": "A barebones boilerplate for building apps with React",
  "engines": {
    "node": "^4.2"
  },
  "scripts": {
    "clean": "rimraf public",
    "conf": "babel-node ./scripts/generate-nginx-conf.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "start:dev": "babel-node ./server.js",
    "start": "npm run start:dev",
    "build:static": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:static"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "author": "Ian Sinnott <ian@iansinnott.com> (https://github.com/iansinnott)",
  "license": "MIT",
  "homepage": "",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "axis": "^0.5.2",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-jest": "^22.4.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "css-loader": "^0.23.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "rimraf": "^2.4.4",
    "rupture": "^0.6.1",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.4.2",
    "url-loader": "^0.5.7",
    "webpack": "1.12.9",
    "webpack-dev-middleware": "1.4.0",
    "webpack-hot-middleware": "2.6.0"
  },
  "dependencies": {
    "whatwg-fetch": "^2.0.3"
  }
}
